//khoi khai bao5
#include <stdio.h>


//khoi ham main
int main(void) {
  int n;
  printf("nhap n =");
  scanf("%d", &n);
  if ( n < 2){
    printf("n khong la so nguyen to");
    return 0;
    }
  int count = 0;
  for(int i = 2; i <= n - 1; i++){
    if(n % i == 0){
      count++;
     }
    }
    if(count == 0){
      printf("\n%d la so nguyen to", n);
    }else
    printf("\n %d khong la so nguyen to", n);   
return 0;
}

